{
  "$schema": "http://json.schemastore.org/template",
  "author": "Microsoft",
  "classifications": ["Universal"],
  "name": "Background Task",
  "shortName": "Background Task",
  "groupIdentity": "ts.Feat.BackgroundTask",
  "identity": "ts.Feat.BackgroundTask",
  "description": "Add an in-process background task ready to run code even while the app is not in the foreground.",
  "tags": {
    "language": "C#",
    "type": "item",
    "ts.type": "feature",
    "ts.projecttype": "all",
    "ts.frontendframework": "CodeBehind|MVVMToolkit",
    "ts.platform": "Uwp",
    "ts.version": "1.0.0",
    "ts.displayOrder": "1",
    "ts.group": "BackgroundWork",
    "ts.multipleInstance": "true",
    "ts.genGroup": "0",
    "ts.rightClickEnabled":"false",
    "ts.outputToParent":"true",
    "ts.exclusions": "ts.Feat.MultiInstance|ts.Feat.MultiInstanceAdvanced"
  },
  "sourceName": "BackgroundTaskFeature",
  "preferNameDirectory": true,
  "PrimaryOutputs": [
    {
      "path": "Param_ProjectName/Services/BackgroundTaskService.cs"
    },
    {
      "path": "Param_ProjectName/BackgroundTasks/BackgroundTask.cs"
    },
    {
      "path": "Param_ProjectName/BackgroundTasks/BackgroundTaskFeature.cs"
    },
    {
      "path": "Param_ProjectName.Core/Helpers/TaskExtensions.cs"
    }
  ],
  "symbols": {
    "wts.projectName": {
      "type": "parameter",
      "replaces": "Param_ProjectName",
      "fileRename": "Param_ProjectName"
    },
    "wts.rootNamespace": {
      "type": "parameter",
      "replaces": "Param_RootNamespace"
    }
  }
}
