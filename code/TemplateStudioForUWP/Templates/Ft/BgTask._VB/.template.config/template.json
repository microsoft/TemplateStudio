{
  "$schema": "http://json.schemastore.org/template",
  "author": "Microsoft Community",
  "classifications": ["Universal"],
  "name": "Background Task",
  "shortName": "Background Task",
  "groupIdentity": "ts.Feat.BackgroundTask.VB",
  "identity": "ts.Feat.BackgroundTask.VB",
  "description": "Add an in-process background task ready to run code even while the app is not in the foreground.",
  "tags": {
    "language": "VisualBasic",
    "type": "item",
    "ts.type": "feature",
    "ts.projecttype": "all",
    "ts.frontendframework": "CodeBehind|MVVMToolkit",
    "ts.platform": "Uwp",
    "ts.version": "1.0.0",
    "ts.displayOrder": "1",
    "ts.group": "BackgroundWork",
    "ts.multipleInstance": "true",
    "ts.genGroup": "0",
    "ts.rightClickEnabled": "false",
    "ts.outputToParent": "true",
    "ts.exclusions": "ts.Feat.MultiInstance.VB|ts.Feat.MultiInstanceAdvanced.VB"

  },
  "sourceName": "BackgroundTaskFeature",
  "preferNameDirectory": true,
  "PrimaryOutputs": [
    {
      "path": "Param_ProjectName/Services/BackgroundTaskService.vb"
    },
    {
      "path": "Param_ProjectName/BackgroundTasks/BackgroundTask.vb"
    },
    {
      "path": "Param_ProjectName/BackgroundTasks/BackgroundTaskFeature.vb"
    },
    {
      "path": "Param_ProjectName.Core/Helpers/TaskExtensions.vb"
    }
  ],
  "symbols": {
    "ts.projectName": {
      "type": "parameter",
      "replaces": "Param_ProjectName",
      "fileRename": "Param_ProjectName"
    },
    "ts.rootNamespace": {
      "type": "parameter",
      "replaces": "Param_RootNamespace"
    }
  }
}
