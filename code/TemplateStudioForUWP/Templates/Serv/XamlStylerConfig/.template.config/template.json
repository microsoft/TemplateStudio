{
  "$schema": "http://json.schemastore.org/template",
  "author": "Microsoft Community",
  "classifications": ["Universal"],
  "name": "XAML Styler Config",
  "shortName": "XAML Styler Config",
  "groupIdentity": "ts.Service.XAMLStyler",
  "identity": "ts.Service.XAMLStyler",
  "description": "Default XAML Styler config file.",
  "tags": {
    "language": "C#",
    "type": "item",
    "ts.type": "service",
    "ts.projecttype": "all",
    "ts.frontendframework": "CodeBehind|Prism|MVVMToolkit",
    "ts.platform": "Uwp",
    "ts.group": "Tools",
    "ts.version": "1.0.0",
    "ts.displayOrder": "1",
    "ts.defaultInstance": "XAMLStyler",
    "ts.multipleInstance": "false",
    "ts.isHidden": "false",
    "ts.genGroup": "0",
    "ts.rightClickEnabled":"true",
    "ts.outputToParent": "true",
    "ts.licenses": "[XAMLStyler](https://marketplace.visualstudio.com/items/TeamXavalon.XAMLStyler/license)"
  },
  "sourceName": "XAMLStylerFeature",
  "preferNameDirectory": true,
  "PrimaryOutputs": [
    {
      "path": "Param_ProjectName/Settings.XamlStyler"
    }
  ],
  "symbols": {
    "ts.projectName": {
      "type": "parameter",
      "replaces": "Param_ProjectName",
      "fileRename": "Param_ProjectName"
    },
    "ts.rootNamespace": {
      "type": "parameter",
      "replaces": "Param_RootNamespace"
    }
  }
}
