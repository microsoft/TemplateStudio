{
  "$schema": "http://json.schemastore.org/template",
  "author": "Microsoft",
  "classifications": ["Universal"],
  "name": "Multiple views",
  "shortName": "Multiple views",
  "groupIdentity": "ts.WPF.Feat.MultiView",
  "identity": "ts.WPF.Feat.MultiView",
  "description": "Enable your users to view independent parts of your app in separate windows.",
  "tags": {
    "language": "C#",
    "type": "item",
    "ts.type": "feature",
    "ts.projecttype": "all",
    "ts.frontendframework": "MVVMToolkit",
    "ts.platform": "Wpf",
    "ts.version": "1.0.0",
    "ts.displayOrder": "2",
    "ts.group": "ApplicationLifecycle",
    "ts.defaultInstance": "MultiView",
    "ts.multipleInstance": "false",
    "ts.genGroup": "0",
    "ts.rightClickEnabled":"true"
  },
  "sourceName": "wts.ItemName",
  "preferNameDirectory": true,
  "PrimaryOutputs": [
    {
      "path": "Contracts/Services/IWindowManagerService.cs"
    },
    {
      "path": "Contracts/Views/IShellDialogWindow.cs"
    },
    {
      "path": "Helpers/WindowExtensions.cs"
    },
    {
      "path": "Services/WindowManagerService.cs"
    },
    {
      "path": "ViewModels/ShellDialogViewModel.cs"
    },
    {
      "path": "Views/ShellDialogWindow.xaml"
    },
    {
      "path": "Views/ShellDialogWindow.xaml.cs"
    }
  ],
  "symbols": {
    "wts.rootNamespace": {
      "type": "parameter",
      "replaces": "Param_RootNamespace"
    },
    "baseclass": {
      "type": "parameter",
      "replaces": "System.ComponentModel.INotifyPropertyChanged"
    },
    "commandclass": {
      "type": "parameter",
      "replaces": "System.Windows.Input.ICommand"
    }
  }
}
