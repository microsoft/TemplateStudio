{
    "$schema": "http://json.schemastore.org/template",
    "author": "Microsoft",
    "classifications": [
      "Universal"
    ],
    "name": "wts.Wpf.Prism.Project.Blank",
    "shortName": "wts.Wpf.Prism.Project.Blank",
    "identity": "wts.Wpf.Prism.Project.Blank",
    "tags": {
      "language": "C#",
      "type": "item",
      "wts.type": "composition",
      "wts.platform" : "Wpf",
      "wts.version": "1.0.0",
      "wts.compositionOrder": "1",
      "wts.compositionFilter": "$frontendframework == Prism & $projectType == Blank & wts.type == project"
    },
    "sourceName": "wts.ItemName",
    "preferNameDirectory": true,
    "PrimaryOutputs": [
      {
        "path": "ViewModels/ShellViewModel.cs"
      },
      {
        "path": "Views/ShellWindow.xaml"
      },
      {
        "path": "Views/ShellWindow.xaml.cs"
      }
    ],
    "symbols": {
      "wts.rootNamespace": {
        "type": "parameter",
        "replaces": "Param_RootNamespace"
      },
      "wts.homePageName": {
        "type":"parameter",
        "replaces":"Param_HomeName"
      },
    }
  }
